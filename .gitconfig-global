# I copy this to ~/.gitconfig but it's named as it is in order to
# prevent git from interpreting it as a .gitconfig scoped to this
# repo.  Usually this file needs some site editing before it's used on
# a particular Windows installation anyway.
[core]
        editor = 'C:\\Program Files\\emacs-24.5-bin-i686-mingw32\\bin\\emacsclientw.exe'
